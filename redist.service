[Unit]
Description=redist service to load a new redist mmdb file
After=network.target
StartLimitIntervalSec=0

[Service]
Type=simple
Restart=always
RestartSec=1
User=root
Environment="NODE_ENV=production"
WorkingDirectory=/home/nbot/IPa

ExecStart=/usr/bin/env npm run preload

[Install]
WantedBy=multi-user.target
